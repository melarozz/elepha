{
  "expo": {
    "name": "react-native-typescript",
    "slug": "react-native-typescript",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assets/images/icon.png",
    "splash": {
      "image": "../assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Template for expo project with TS, Storybook, Reg-Suit"
  }
}
